{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "env": {
        "NODE_ENV": "development"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "env": {
        "NODE_ENV": "preview"
      },
      "distribution": "internal"
    },
    "production": {
      "env": {
        "NODE_ENV": "production"
      },
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "production"
      }
    }
  }
}
